{
  "dashboard": {
    "title": "TP2 DevOps - Application Overview",
    "tags": ["tp2devops", "application"],
    "timezone": "browser",
    "panels": [
      {
        "id": 1,
        "title": "HTTP Requests Total",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(tp2devops_http_requests_total[5m])",
            "legendFormat": "{{method}} {{route}} - {{status_code}}"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 0}
      },
      {
        "id": 2,
        "title": "HTTP Request Duration (p95)",
        "type": "graph",
        "targets": [
          {
            "expr": "histogram_quantile(0.95, rate(tp2devops_http_request_duration_seconds_bucket[5m]))",
            "legendFormat": "{{method}} {{route}}"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 0}
      },
      {
        "id": 3,
        "title": "Active Todos",
        "type": "stat",
        "targets": [
          {
            "expr": "tp2devops_todos_active"
          }
        ],
        "gridPos": {"h": 4, "w": 6, "x": 0, "y": 8}
      },
      {
        "id": 4,
        "title": "Completed Todos",
        "type": "stat",
        "targets": [
          {
            "expr": "tp2devops_todos_completed"
          }
        ],
        "gridPos": {"h": 4, "w": 6, "x": 6, "y": 8}
      },
      {
        "id": 5,
        "title": "Todo Operations",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(tp2devops_todo_operations_total[5m])",
            "legendFormat": "{{operation}}"
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 8}
      },
      {
        "id": 6,
        "title": "Recent Logs",
        "type": "logs",
        "targets": [
          {
            "expr": "{namespace=\"tp2devops\"} |= ``",
            "refId": "A"
          }
        ],
        "gridPos": {"h": 8, "w": 24, "x": 0, "y": 16}
      }
    ],
    "refresh": "10s",
    "schemaVersion": 38,
    "style": "dark",
    "time": {
      "from": "now-1h",
      "to": "now"
    }
  }
}

